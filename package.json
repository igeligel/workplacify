{
  "name": "workplaciyf",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "prisma-studio": "prisma studio",
    "db-nuke": "docker compose down --volumes --remove-orphans",
    "db-up": "docker compose up -d",
    "db-seed": "prisma db seed",
    "db-reset": "prisma migrate dev reset",
    "dx:next": "run-s db-up migrate-dev db-seed && next dev --experimental-https",
    "dx": "run-p dx:* --print-label",
    "dev": "npm run dx:next",
    "prebuild": "run-s generate migrate",
    "build": "next build",
    "build-ci": "next build",
    "start": "next start",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "npm run lint --fix",
    "migrate-dev": "prisma migrate dev",
    "prisma-case-format": "cross-env prisma-case-format --file ./prisma/schema.prisma --map-table-case=snake --map-field-case=snake --map-enum-case=snake",
    "migrate": "prisma migrate deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "tsc": "tsc --noEmit",
    "postinstall": "npm run generate",
    "sitemap": "next-sitemap",
    "postbuild": "next-sitemap",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=20.8.1"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.8.0",
    "@chakra-ui/react": "^3.24.2",
    "@cloudinary/react": "^1.13.1",
    "@cloudinary/url-gen": "^1.21.0",
    "@emotion/react": "^11.14.0",
    "@prisma/client": "^6.7.0",
    "@react-pdf/renderer": "^4.3.0",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/next": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@uidotdev/usehooks": "^2.4.1",
    "boring-avatars": "^2.0.1",
    "change-case": "^5.4.4",
    "cloudinary": "^2.6.0",
    "clsx": "^2.1.1",
    "datauri": "^4.1.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "deepmerge": "^4.3.1",
    "discord.js": "^14.18.0",
    "html-to-image": "^1.11.13",
    "modern-screenshot": "^4.6.6",
    "multer": "^1.4.5-lts.1",
    "next": "^15.1.3",
    "next-auth": "^4.24.11",
    "next-connect": "^1.0.0",
    "next-intl": "^3.26.5",
    "next-seo": "^6.6.0",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "posthog-js": "^1.260.2",
    "prisma": "^6.7.0",
    "react": "^18.3.1",
    "react-day-picker": "^9.6.2",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-medium-image-zoom": "^5.3.0",
    "react-qr-code": "^2.0.18",
    "react-timezone-select": "^3.2.8",
    "react-use": "^17.6.0",
    "react-zoom-pan-pinch": "^3.7.0",
    "superjson": "^2.2.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@playwright/test": "^1.51.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.3",
    "@types/react": "^18.3.18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prisma-case-format": "^2.1.0-beta.0",
    "start-server-and-test": "^2.0.11",
    "tsx": "^4.20.4",
    "typescript": "^5.8.2",
    "vite": "^5.4.14",
    "vitest": "^1.6.1"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
