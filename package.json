{
  "name": "workplaciyf",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "prisma-studio": "prisma studio",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-reset": "prisma migrate dev reset",
    "dx:next": "run-s db-up migrate-dev db-seed && next dev --experimental-https",
    "dx": "run-p dx:* --print-label",
    "dev": "npm run dx:next",
    "prebuild": "run-s generate migrate",
    "build": "next build",
    "build-ci": "next build",
    "start": "next start",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "npm run lint --fix",
    "migrate-dev": "prisma migrate dev",
    "prisma-case-format": "cross-env prisma-case-format --file ./prisma/schema.prisma --map-table-case=snake --map-field-case=snake --map-enum-case=snake",
    "migrate": "prisma migrate deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "tsc": "tsc --noEmit",
    "postinstall": "npm run generate"
  },
  "engines": {
    "node": ">=20.8.1"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.5.0",
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@cloudinary/react": "^1.12.0",
    "@cloudinary/url-gen": "^1.16.1",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@prisma/client": "^5.9.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/next": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "cloudinary": "^1.41.3",
    "clsx": "^2.1.0",
    "datauri": "^4.1.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "discord.js": "^14.14.1",
    "framer-motion": "^11.0.14",
    "multer": "^1.4.5-lts.1",
    "next": "^14.1.0",
    "next-auth": "^4.24.7",
    "next-connect": "^1.0.0",
    "next-seo": "^6.4.0",
    "posthog-js": "^1.110.0",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-timezone-select": "^3.2.3",
    "react-use": "^17.5.0",
    "react-zoom-pan-pinch": "^3.4.3",
    "superjson": "^2.2.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.42.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.55",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "prettier": "^3.2.5",
    "prisma": "^5.9.1",
    "prisma-case-format": "^2.1.0-beta.0",
    "start-server-and-test": "^2.0.3",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
